{
  "name": "eslint-plugin-mui-import-order",
  "version": "0.0.1",
  "description": "an ESLint plugin that transforms barrel imports into default imports and enforces import sorting.",
  "main": "lib/index.js",
  "exports": {
    ".": {
      "require": "./lib/index.js",
      "import": "./lib/index.mjs"
    }
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "test": "node tests/no-barrel-import.test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/taylous/eslint-plugin-mui-import-order.git"
  },
  "keywords": [
    "eslint",
    "plugin",
    "mui"
  ],
  "author": "taylous",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/taylous/eslint-plugin-mui-import-order/issues"
  },
  "homepage": "https://github.com/taylous/eslint-plugin-mui-import-order#readme",
  "peerDependencies": {
    "eslint": ">=9.0.0 <10.0.0"
  },
  "devDependencies": {
    "eslint": "^9.37.0"
  }
}
